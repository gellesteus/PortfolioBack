{
  "name": "portfolio-dnd-app-backend",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node --inspect=5998 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "test": "jest --detectOpenHandles",
    "prewebpack": "npm run build",
    "webpack": "webpack",
    "postwebpack": "node ./dist/cli/pack.js",
    "prebuild": "node ./dist/cli/clean.js",
    "build": "tsc",
    "generateKey": "node ./dist/cli/generateKey.js",
    "jsdoc:generate": "npm run build && jsdoc  -R ./readme.md -r ./dist",
    "jsdoc:config": "jsdoc -c jsdoc.conf",
    "packed": "node ./bundle/server/server.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/cli/*.ts"
    ],
    "watch": [
      "src",
      "node_modules"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "keywords": [],
  "author": "Mark Kohler",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "express-mung": "^0.5.1",
    "express-validator": "^6.1.1",
    "fs-extra": "^8.1.0",
    "handy-redis": "^1.6.2",
    "jsdoc-route-plugin": "^0.1.0",
    "moment": "^2.24.0",
    "mongoose": "^5.6.8",
    "redis": "^2.8.0",
    "ws": "^7.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-fileupload": "^1.1.0",
    "@types/express-mung": "^0.5.2",
    "@types/express-validator": "^3.0.0",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.18",
    "@types/mongoose": "^5.5.15",
    "@types/node": "^12.7.2",
    "@types/redis": "^2.8.13",
    "@types/ws": "^6.0.3",
    "concurrently": "^4.1.2",
    "jest": "24.8.0",
    "jsdoc": "^3.6.3",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2",
    "webpack": "4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
